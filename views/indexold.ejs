    <form id="regForm" action="#">
        <h1 class="text-center"><b>REGISTER</b></h1>

        <div class="tab">
            <input type="text" placeholder="Register Number" class="my-2">
            <input type="email" placeholder="Email ID" class="my-2">
            <input type="text" placeholder="Full Name" class="my-2">
            <input type="tel" placeholder="Mobile Number" class="my-2">
        </div>
        <div class="tab">
            <input placeholder="Password" type="password" class="my-2">
            <input placeholder="Confirm Password" type="password" class="my-2">
            <input placeholder="Gender" type="text" class="my-2">
            <input placeholder="Room Number" type="text" class="my-2">
        </div>
        <div class="tab">
            <div class="mt-5">
                <p class="text-center"><b>Hi Prerna Sultania !!</b></p>
                <p class="text-center"><b>Last Step..</b></p>
            </div>
            <div class="text-center">
                <a href="#"><img src="Register Buttton.png" class="img-fluid register"
                        style="border-radius: 100%;border:none;" /></a>
            </div>
        </div>
        <div style="overflow:auto;" class="mt-3">
            <button type="button" id="prevBtn" onclick="nextPrev(-1)" class="my-4"
                style="float: left">Back</button>
            <button type="button" id="nextBtn" onclick="nextPrev(1)" class="my-4"
                style="float: right">Next</button>
        </div>

        <div style="text-align:center;">
            <span class="step"></span>
            <span class="step"></span>
            <span class="step"></span>
        </div>
    </form>

